                        </li> 
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div>
            <h1>PROGRAMS : @ViewData["Title"]</h1>
            <br />
            <h2>STUDENT NAME: Gurkomal Singh</h2>
        <main role="main"> 
            @RenderBody()
        </main>
